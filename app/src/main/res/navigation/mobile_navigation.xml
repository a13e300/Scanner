<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/scanning_navigation">

    <fragment
        android:id="@+id/navigation_generator"
        android:name="io.github.a13e300.scanner.ui.generator.GeneratorFragment"
        android:label="@string/title_generator"
        tools:layout="@layout/fragment_generator" />

    <include app:graph="@navigation/scanning_navigation" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="io.github.a13e300.scanner.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
</navigation>